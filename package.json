{
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=client babel-node src/server/serverInit.js --extensions '.js','.ts','.tsx'",
    "build-vendor": "NODE_ENV=production BABEL_ENV=client webpack --config webpack.vendor.config.js",
    "build-vendor-dev": "NODE_ENV=development BABEL_ENV=client webpack --config webpack.vendor.config.js",
    "build-webpack": "NODE_ENV=production BABEL_ENV=client webpack",
    "build-babel": "NODE_ENV=production BABEL_ENV=server babel src -d dist --copy-files --extensions '.js','.ts','.tsx'",
    "build": "npm run build-webpack && npm run build-babel",
    "serve": "NODE_ENV=production pm2 start dist/server/serverInit.js --name nightingale"
